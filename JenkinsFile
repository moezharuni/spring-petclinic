node
    stage('source code'){
            steps {
               git url: 'https://github.com/spring-projects/spring-petclinic.git'
               branch: 'REL_V1'
            }
        stage('build') {
            sh 'mvn package'
            }
        stage('Archive the Artifacts'){
            archive 'target/*.jar'
            }
        stage('Junit Results'){
                junit '**/surefire-reports/*.xml'
            }    
    }